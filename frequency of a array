#include <bits/stdc++.h>
using namespace std;

int main() {
    // Write C++ code here
    int n;
    cin>>n; 
    int arr[n];
    map<int, int> m;
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
    
    for(int i=0; i<n; i++){
        m[arr[i]]++;
    }
    for(auto it:m){
        cout<<it.first<<" "<<it.second<<"\n";
    }
    

    return 0;
}
